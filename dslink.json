{
  "name": "dslink-scala-keyence-hostlink",
  "version": "0.1",
  "description": "Acquiring data from Keyence KV-7500 via HostLink protocol",
  "license": "Cisco Sample Code License",
  "author": {
    "name": "Naoto Shimazaki",
    "email": "nshimaza@cisco.com"
  },
  "main": "bin/dslink-scala-keyence-hostlink",
  "repository": {
    "type": "git",
    "url": "https://github.com/nshimaza/dslink-scala-keyence-hostlink"
  },
  "bugs": {
    "url": "https://github.com/nshimaza/dslink-scala-keyence-hostlink/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "KeyenceHostLink"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "com.example.KeyenceHostLinkDSLinkHandler"
    }
  }
}
